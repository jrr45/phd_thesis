  author = {Padmanabhan, Haricharan and Park, Yoonsang and Puggioni, Danilo and Yuan, Yakun and Cao, Yanwei and Gasparov, Lev and Shi, Youguo and Chakhalian, Jak and Rondinelli, James M. and Gopalan, Venkatraman},
  year = {2018},
