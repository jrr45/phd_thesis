  author = {Jin, Feng and Wang, Le and Zhang, Anmin and Ji, Jianting and Shi, Youguo and Wang, Xiaoqun and Yu, Rong and Zhang, Jiandi and Plummer, E. W. and Zhang, Qingming},
  year = {2019},
